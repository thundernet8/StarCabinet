{
    "presets": ["react", "es2015", "es2017", "stage-2"],
    "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-object-rest-spread",
        [
            "transform-async-to-module-method",
            {
                "module": "bluebird",
                "method": "coroutine"
            }
        ],
        ["import", { "libraryName": "antd", "style": true }]
    ],
    "comments": false
}
